{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": false,
    "lib": ["ES6"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "strict": true,
    "allowJs": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"]
}
